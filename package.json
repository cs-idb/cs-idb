{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "routify -b && rollup -c",
    "dev": "run-p watch:routify watch:rollup",
    "watch:routify": "routify",
    "watch:rollup": "rollup -c -w",
    "start": "sirv public --single",
    "lint": "eslint --fix 'src/**/*.{js,svelte}'",
    "format": "prettier --write 'src/**/*.{js,svelte}'"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "autoprefixer": "^9.8.4",
    "eslint": "^7.3.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "js-sha1": "^0.6.0",
    "json-server": "^0.16.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^1.20.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^5.1.2",
    "sirv-cli": "^0.4.6",
    "svelte": "^3.49.0",
    "svelte-preprocess": "^3.9.11"
  },
  "dependencies": {
    "@sveltech/routify": "^1.7.13",
    "localforage": "^1.7.4",
    "materialize-css": "^1.0.0-rc.2",
    "npm-run-all": "^4.1.5",
    "svelte-infinite-scroll": "^1.4.0",
    "svelte-select": "^3.8.1"
  }
}
